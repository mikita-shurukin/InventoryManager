@model InventoryManager.Models.Requests.Warehouse.CreateWarehouseRequest

<form id="createWarehouseForm" method="post" action="/api/Warehouses/Create">
    <div class="mt-3">
        <label class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name">
    </div>
    <div class="mt-3">
        <label class="form-label">Storage Location</label>
        <input type="text" class="form-control" asp-for="StorageLocation">
    </div>
    <div class="mt-3">
        <label class="form-label">Contact Person</label>
        <input type="text" class="form-control" asp-for="ContactPerson">
    </div>
    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Create</button>
    </div>
</form>
